 	//Three errors below
 	try {
 	// variables
 	let x = 5; //added semicolon so it run correctly
 	let y = 3; //added semicolon so it run correctly
 	const sum = x - y; //added semicolon so it run correctly
 	 
 	// if statement ===
 	if (sum === 2) throw 'Equal Value';// removed the s from throw and added  a semicolon
 	 
 	// output
 	console.log(sum); //added semicolon
 	} catch (err) { // removed s from err because it wouldn't match err
